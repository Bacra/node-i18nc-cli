{
  "name": "i18nc",
  "version": "4.6.0",
  "description": "i18nc",
  "main": "index.js",
  "bin": {
    "i18nc": "bin/i18nc.js"
  },
  "scripts": {
    "prepublish": "npm ls",
    "lint": "eslint .",
    "test": "cross-env DEBUG=i18nc* mocha test/test_*",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha -- test/test_* --reporter dot",
    "test-e2e": "karma start --",
    "test-e2e-dev": "cross-env DEBUG=i18nc-core* karma start --auto-watch --no-single-run --browsers=Chrome",
    "test-e2e-sauce": "karma start -- sauce",
    "test-travis": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- test/test_* --reporter dot",
    "test-build": "node test/prev_test_files; cross-env TEST_BUILD=true mocha test/test_*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Bacra/node-i18nc-cli.git"
  },
  "keywords": [
    "i18n",
    "cli",
    "js"
  ],
  "author": "Bacra Woo <bacra.woo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bacra/node-i18nc-cli/issues"
  },
  "homepage": "https://github.com/Bacra/node-i18nc-cli",
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "debug": "^3.1.0",
    "extend": "^3.0.1",
    "glob": "^7.1.2",
    "i18nc-core": "^10.4.1",
    "i18nc-po": "^2.4.1",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "strip-bom": "^3.0.0",
    "table": "4.0.3"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.1.0",
    "eslint-config-brcjs": "^0.2.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "karma": "^2.0.4",
    "karma-config-brcjs": "^1.0.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2"
  }
}
